---
import { listYourBusinessLink } from "../config/links";
---
<section class="h-full px-24 w-full rounded-2xl overflow-hidden relative min-h-[340px] flex items-stretch" style="background: #fff;">
  <!-- Decorative lines -->
  <div class="absolute top-0 left-1/2 -translate-x-1/2 w-[50vw] h-[2px] rounded-full bg-gradient-to-r from-transparent via-[#1AF4FE] to-transparent z-20"></div>
  <div class="absolute bottom-0 left-1/2 -translate-x-1/2 w-[50vw] h-[2px] rounded-full bg-gradient-to-r from-transparent via-[#1AF4FE] to-transparent z-20"></div>
  <!-- <div class="absolute top-0 right-8 w-16 h-[2px] rounded-full bg-gradient-to-l from-[#1AF4FE] to-transparent z-20"></div> -->
  <!-- Blurred ellipses -->
  <div class="absolute z-0 pointer-events-none w-full h-full">
    <!-- Bottom left ellipse -->
    <div class="absolute left-32 bottom-[-60px] w-[300px] h-[180px] rounded-full bg-[#1AF4FE] opacity-30 blur-[100px]"></div>
    <!-- Top right ellipse -->
    <div class="absolute right-32 top-[-60px] w-[220px] h-[140px] rounded-full bg-[#1AF4FE] opacity-30 blur-[100px]"></div>
  </div>
  <!-- Content -->
  <div class="relative z-20 flex flex-col items-start justify-between h-full py-12 lg:py-20 max-w-3xl gap-12">
    <div class="flex flex-col">
        <h2 class="text-2xl md:text-4xl font-[Noto_Serif_Display] mb-4 leading-tight text-[#0094BD]">Are you a shop owner?</h2>
      <p class="text-xl md:text-3xl font-[Noto_Serif_Display] mb-8 leading-tight text-[#004346]">Sell to more people</p>
    </div>
    <a href={listYourBusinessLink} target="_blank" rel="noopener" class="flex items-center gap-3 bg-[#155e75] hover:bg-[#204040] text-white text-md font-medium px-16 py-3 rounded-xl shadow transition-colors hover:bg-linear-to-tl hover:from-[#0E5376] hover:to-[#1AF4FE]  transition-all duration-500 hover:scale-105">
